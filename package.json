{
  "name": "Devsu-test",
  "version": "1.0.0",
  "description": "this repository was created for a test of the Devsu company",
  "main": "index.js",
  "scripts": {
    "check:code": "eslint **/*.js",
    "check:features": "gherkin-lint",
    "test": "cucumber-js --config cucumber.js",
    "test:bug": "cucumber-js -p default --tags @bug",
    "test:ui": "cucumber-js -p default --tags \"(@ui and not @bug)\"",
    "test:testing": "cucumber-js -p default --tags @testing",
    "test:jest": "jest --config ./jest.config.js"
  },
  "author": "Israel Zambrana",
  "license": "ISC",
  "dependencies": {
    "@cucumber/cucumber": "^9.4.0",
    "@cucumber/pretty-formatter": "^1.0.0",
    "@faker-js/faker": "^8.0.2",
    "chromedriver": "^126.0.5",
    "cucumber-html-reporter": "^6.0.0",
    "dotenv": "^16.3.1",
    "eslint": "^8.47.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "expect": "^29.6.3",
    "gherkin-lint": "^4.2.2",
    "jest": "^29.6.3",
    "jest-html-reporters": "^3.1.4",
    "jest-junit": "^16.0.0",
    "jsonschema": "^1.4.1",
    "pino": "^8.15.0",
    "pino-pretty": "^10.2.0",
    "selenium-webdriver": "^4.10.0"
  }
}
